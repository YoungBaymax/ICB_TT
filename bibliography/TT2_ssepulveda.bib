	pages = {1760--1765},
	file = {IEEE Xplore Full Text PDF:C\:\\Users\\SSepulveda\\AppData\\Roaming\\Zotero\\Zotero\\Profiles\\pyre7rqa.default\\zotero\\storage\\8T6R27N3\\Huang and Hsu - 2005 - Development of a wearable biomedical health-care s.pdf:application/pdf}
