	pages = {287--294},
	file = {IEEE Xplore Full Text PDF:C\:\\Users\\SSepulveda\\AppData\\Roaming\\Zotero\\Zotero\\Profiles\\pyre7rqa.default\\zotero\\storage\\IHQ6JMPB\\Huang et al. - 2013 - Robust Human Body Shape and Pose Tracking.pdf:application/pdf}
