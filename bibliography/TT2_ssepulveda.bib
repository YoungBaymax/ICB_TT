	keywords = {Aging, Clinics, Database, Datenbank, Geriatrics/Gerontology, Internal Medicine, Kliniken, Labor, Laboratory, Methoden, Methods, Risikobewertung, Risk assessment, Social Sciences, general},
	pages = {706--719},
	file = {Full Text PDF:C\:\\Users\\SSepulveda\\AppData\\Roaming\\Zotero\\Zotero\\Profiles\\pyre7rqa.default\\zotero\\storage\\2FWV6ZZH\\Schwickert et al. - 2013 - Fall detection with body-worn sensors.pdf:application/pdf;Snapshot:C\:\\Users\\SSepulveda\\AppData\\Roaming\\Zotero\\Zotero\\Profiles\\pyre7rqa.default\\zotero\\storage\\RR2FDHHC\\s00391-013-0559-8.html:text/html}
