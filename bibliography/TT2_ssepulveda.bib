	pages = {1754--1759},
	file = {IEEE Xplore Abstract Record:C\:\\Users\\SSepulveda\\AppData\\Roaming\\Zotero\\Zotero\\Profiles\\pyre7rqa.default\\zotero\\storage\\EDDCPSND\\articleDetails.html:text/html;IEEE Xplore Full Text PDF:C\:\\Users\\SSepulveda\\AppData\\Roaming\\Zotero\\Zotero\\Profiles\\pyre7rqa.default\\zotero\\storage\\BN9ENN5G\\Khoder et al. - 2008 - Inertial navigation attitude velocity and position.pdf:application/pdf}
