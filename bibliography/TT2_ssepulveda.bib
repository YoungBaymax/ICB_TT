	pages = {2653--2657},
	file = {IEEE Xplore Full Text PDF:C\:\\Users\\SSepulveda\\AppData\\Roaming\\Zotero\\Zotero\\Profiles\\pyre7rqa.default\\zotero\\storage\\ABTJPCQE\\Xiong et al. - 2011 - A novel HCI based on EMG and IMU.pdf:application/pdf}
}

@inproceedings{terrill_measuring_2013,
	title = {Measuring leg movements during sleep using accelerometry: Comparison with {EMG} and piezo-electric scored events},
	shorttitle = {Measuring leg movements during sleep using accelerometry},
	doi = {10.1109/EMBC.2013.6611134},
	abstract = {Periodic Limb Movements during Sleep ({PLMS}) can cause significant disturbance to sleep, resulting in daytime sleepiness and reduced quality of life. In conventional clinical practice, {PLMS} are measured using overnight electromyogram ({EMG}) of the tibialis anterior muscle, although historically they have also been measured using piezo-electric gauges placed over the muscle. However, {PLMS} counts ({PLM} index) do not correlate well with clinical symptomology. In this study, we propose that because {EMG} and piezo derived signals measure muscle activation rather than actual movement, they may count events with no appreciable movement of the limb and therefore no contribution to sleep disturbance. The aim of this study is thus to determine the percentage of clinically scored limb movements which are not associated with movement of the great toe measured using accelerometry. 9 participants were studied simultaneously with an overnight diagnostic polysomnogram (including {EMG} and piezo instrumentation of the right leg) and high temporal resolution accelerometry of the right great toe. Limb movements were scored, and peak acceleration during each scored movement was quantified. Across the participant population, 54.9\% (range: 26.7-76.3) and 39.0\% (range: 4.8-69.6) of limb movements scored using piezo and {EMG} instrumentation respectively, were not associated with toe movement measured with accelerometry. If sleep disturbance is the consequence of the limb movements, these results may explain why conventional piezo or {EMG} derived {PLMI} is poorly correlated with clinical symptomology.},
	author = {Terrill, P.I. and Leong, M. and Barton, K. and Freakley, C. and Downey, C. and Vanniekerk, M. and Jorgensen, G. and Douglas, J.},
	month = jul,
	year = {2013},
	keywords = {Accelerometers, bioelectric phenomena, biomechanics, clinically scored limb movements, clinical symptomology, diagnostic polysomnogram, electromyogram, electromyography, {EMG} derived {PLMI}, {EMG} instrumentation, high-temporal resolution accelerometry, Instruments, Legged locomotion, leg movements, Medical diagnostic imaging, muscle, muscle activation, patient diagnosis, periodic limb movement-during-sleep, piezo derived {PLMI}, piezo-derived signals, piezo-electric gauges, piezoelectricity, piezo-electric scored events, piezo instrumentation, {PLMS}, sleep, Sleep apnea, sleep disturbance, tibialis anterior muscle, toe movement},
	pages = {6862--6865},
	file = {IEEE Xplore Full Text PDF:C\:\\Users\\SSepulveda\\AppData\\Roaming\\Zotero\\Zotero\\Profiles\\pyre7rqa.default\\zotero\\storage\\TCDPS73M\\Terrill et al. - 2013 - Measuring leg movements during sleep using acceler.pdf:application/pdf}
}

@article{yun_cho_adaptive_2013,
	title = {Adaptive Redundant Inertial Measurement Unit/Global Positioning System Integration Filter Structure for Fault Tolerant Navigation},
	volume = {136},
	issn = {0022-0434},
	url = {http://dx.doi.org/10.1115/1.4025753},
	doi = {10.1115/1.4025753},
	abstract = {This paper proposes a new adaptive filter structure containing two subfilters, a fault detection and isolation ({FDI}) filter, and a main-filter for redundant inertial measurement unit ({RIMU})/global positioning system ({GPS}) integrated navigation system tolerant toward faults of inertial sensors. The purpose of subfilters is compensation of sensor level bias ({SLB}) of the {RIMU} during in-flight alignment for successful {FDI} processing. Also, that of the main-filter is providing of error compensated navigation solution based on the good {FDI} result. To achieve these purposes, two coordinate frames, sub-body frame ({SBF}) and master-body frame ({MBF}), are defined first. Then, two different error models for the {RIMU} are formulated in the {SBF} for sensor level compensation and in the {MBF} for module level compensation, respectively. Based on the formulated error models, an adaptive filter structure is designed. Some numerical simulations are performed to validate the performance of the proposed adaptive filter structure.},
	number = {2},
	urldate = {2014-04-16},
