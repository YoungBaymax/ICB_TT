	pages = {1020--1027},
	file = {IEEE Xplore Full Text PDF:C\:\\Users\\SSepulveda\\AppData\\Roaming\\Zotero\\Zotero\\Profiles\\pyre7rqa.default\\zotero\\storage\\6Z4F2C5F\\Dadashi et al. - 2014 - Estimation of Front-Crawl Energy Expenditure Using.pdf:application/pdf}
}

@article{xin_development_2012,
