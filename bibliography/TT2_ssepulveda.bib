	pages = {13--18},
	file = {IEEE Xplore Full Text PDF:C\:\\Users\\SSepulveda\\AppData\\Roaming\\Zotero\\Zotero\\Profiles\\pyre7rqa.default\\zotero\\storage\\ZKH5T2MD\\Stubberud and Stubberud - 2008 - A Signal Processing Technique for Improving the Ac.pdf:application/pdf}
