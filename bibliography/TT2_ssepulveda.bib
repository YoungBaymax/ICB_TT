	journal = {Clinical Nutrition},
	author = {Zhao, Di and Xian, Xiaojun and Terrera, Mirna and Krishnan, Ranganath and Miller, Dylan and Bridgeman, Devon and Tao, Kevin and Zhang, Lihua and Tsow, Francis and Forzani, Erica S. and Tao, Nongjian},
