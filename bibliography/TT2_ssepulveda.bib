	pages = {705--708 Vol.1},
	file = {IEEE Xplore Abstract Record:C\:\\Users\\SSepulveda\\AppData\\Roaming\\Zotero\\Zotero\\Profiles\\pyre7rqa.default\\zotero\\storage\\5ZPI77AE\\articleDetails.html:text/html;IEEE Xplore Full Text PDF:C\:\\Users\\SSepulveda\\AppData\\Roaming\\Zotero\\Zotero\\Profiles\\pyre7rqa.default\\zotero\\storage\\F3K5CCAG\\McDannold et al. - 2004 - MRI-based thermometry and thermal dosimetry during.pdf:application/pdf}
