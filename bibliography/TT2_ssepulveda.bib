	month = jun,
	year = {2009},
	keywords = {3D surface, Animals, Deformable models, Humans, image sequences, Joints, joint skeleton tracking, motion capture, motion estimation, multiview image sequence, multiview video sequence, Optimization, pose estimation, Skeleton, skeleton-based pose estimation, surface estimation, surface fitting, Tracking, Video sequences, video signal processing},
