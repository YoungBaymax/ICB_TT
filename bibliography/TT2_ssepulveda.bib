@article{senneville_mr_2007,
	title = {{MR} thermometry for monitoring tumor ablation},
	volume = {17},
	issn = {0938-7994, 1432-1084},
	url = {http://link.springer.com/article/10.1007/s00330-007-0646-6},
	doi = {10.1007/s00330-007-0646-6},
	abstract = {Local thermal therapies are increasingly used in the clinic for tissue ablation. During energy deposition, the actual tissue temperature is difficult to estimate since physiological processes may modify local heat conduction and energy absorption. Blood flow may increase during temperature increase and thus change heat conduction. In order to improve the therapeutic efficiency and the safety of the intervention, mapping of temperature and thermal dose appear to offer the best strategy to optimize such interventions and to provide therapy endpoints. {MRI} can be used to monitor local temperature changes during thermal therapies. On-line availability of dynamic temperature mapping allows prediction of tissue death during the intervention based on semi-empirical thermal dose calculations. Much progress has been made recently in {MR} thermometry research, and some applications are appearing in the clinic. In this paper, the principles of {MRI} temperature mapping are described with special emphasis on methods employing the temperature dependency of the water proton resonance frequency. Then, the prospects and requirements for widespread applications of {MR} thermometry in the clinic are evaluated.},
	language = {en},
	number = {9},
	urldate = {2014-03-25},
	journal = {European Radiology},
	author = {Senneville, Baudouin Denis de and Mougenot, Charles and Quesson, Bruno and Dragonu, Iulius and Grenier, Nicolas and Moonen, Chrit T. W.},
	month = sep,
	year = {2007},
	keywords = {Diagnostic Radiology, Imaging / Radiology, Internal Medicine, Interventional Radiology, mri, Neuroradiology, Therapy, thermometry, Ultrasound},
	pages = {2401--2410},
	file = {Full Text PDF:C\:\\Users\\SSepulveda\\AppData\\Roaming\\Zotero\\Zotero\\Profiles\\pyre7rqa.default\\zotero\\storage\\HPTXJ6RZ\\Senneville et al. - 2007 - MR thermometry for monitoring tumor ablation.pdf:application/pdf;Snapshot:C\:\\Users\\SSepulveda\\AppData\\Roaming\\Zotero\\Zotero\\Profiles\\pyre7rqa.default\\zotero\\storage\\RTEQSUUI\\s00330-007-0646-6.html:text/html}
}