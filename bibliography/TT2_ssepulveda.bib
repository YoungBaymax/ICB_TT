	note = {{WOS}:000330798100009},
	keywords = {brain   temperature, contrast agent, frequency-shift, in-vivo, ischemic-stroke, magnetic-resonance-spectroscopy, {MRI} thermometry, proton chemical-shift, proton   resonance frequency ({PRF}), temperature mapping, thermometry, thermosensitive liposomes, tissue   temperature, water direct saturation},
