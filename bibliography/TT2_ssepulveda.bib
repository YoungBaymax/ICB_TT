@inproceedings{khoder_quaternion_2008,
	title = {Quaternion Unscented Kalman Filtering for integrated Inertial Navigation and {GPS}},
	abstract = {In this paper, a scaled unscented Kalman filter ({SUKF}) based on the quaternion concept is designed for integrating inertial navigation system ({INS}) aided by {GPS} measurements under large attitude error conditions. In this feedback filter, only the bias effects are considered to be independent states and are used to compensate for navigation errors. To preserve the nonlinear nature of the unit quaternion, the weighted mean computation for quaternions is derived in rotational space as a barycentric mean with renormalization and a multiplicative quaternion-error is used for predicted covariance computation of the quaternion because it represents the distance from the predicted mean quaternion. The updates are performed using quaternion multiplication which guarantees that quaternion normalization is maintained in the filter. Since the quaternion process noise increases the uncertainty in attitude orientation, modeling it as a rotation vector is considered. Simulation and experimental results indicate a satisfactory performance of the newly developed model.},
	author = {Khoder, W. and Fassinut-Mombot, B. and Benjelloun, M.},
	month = jun,
	year = {2008},
	keywords = {barycentric mean, bias effects, covariance analysis, feedback, feedback filter, Global positioning system, {GPS}, inertial navigation, Inertial navigation system, Inertial sensor model, integrated inertial navigation, Kalman filters, large attitude error conditions, predicted covariance computation, predicted mean quaternion, Quaternion attitude parameterization, quaternion multiplication, quaternion normalization, quaternion unscented Kalman filtering, Rotation vector, Scaled Augmented Unscented Kalman Filter ({SUKF}), Scaled Sigma Points, scaled unscented Kalman filter, weighted mean computation},
	pages = {1--8},
	file = {IEEE Xplore Abstract Record:C\:\\Users\\SSepulveda\\AppData\\Roaming\\Zotero\\Zotero\\Profiles\\pyre7rqa.default\\zotero\\storage\\RH52ZBEQ\\articleDetails.html:text/html;IEEE Xplore Full Text PDF:C\:\\Users\\SSepulveda\\AppData\\Roaming\\Zotero\\Zotero\\Profiles\\pyre7rqa.default\\zotero\\storage\\4F3QZBTW\\Khoder et al. - 2008 - Quaternion Unscented Kalman Filtering for integrat.pdf:application/pdf}
