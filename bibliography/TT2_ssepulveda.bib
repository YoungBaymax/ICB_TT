@article{sabatini_quaternion-based_2005,
	title = {Quaternion-based strap-down integration method for applications of inertial sensing to gait analysis},
	volume = {43},
	issn = {0140-0118, 1741-0444},
	url = {http://link.springer.com/article/10.1007/BF02345128},
	doi = {10.1007/BF02345128},
	abstract = {The proposed strap-down integration method exploits the cyclical nature of human gait: during the gait swing phase, the quaternion-based attitude representation is integrated using a gyroscope from initial conditions that are determined during stance by an accelerometer. Positioning requires double time integration of the gravity-compensated accelerometer signals during swing. An interpolation technique applied to attitude quaternions was developed to improve the accuracy of orientation and positioning estimates by accounting for the effect of sensor bias and scale factor drifts. A simulation environment was developed for the analysis and testing of the proposed algorithm on a synthetic movement trajectory. The aim was to define the true attitude and positioning used in the computation of estimation errors. By thermal modelling, the changes of bias and scale factor of the inertial sensors, calibrated at a single reference temperature, were analysed over a range of ±10°C, for measurement noise standard deviations up to σg = 2.5° s−1 (gyroscope) and σa = 0.05 m s−1 (accelerometer). The compensation technique reduced the maximum root mean square errors ({RMSEs}) to: {RMSEθ}=14.6° (orientation) and {RMSEd}=17.7 cm (positioning) for an integration interval of one gait cycle (an improvement of 3° and 7 cm); {RMSEθ}=14.8° and {RMSEd}=30.0 cm for an integration interval of two gait cycles (an improvement of 11° and 262 cm).},
	language = {en},
	number = {1},
	urldate = {2014-03-13},
	journal = {Medical and Biological Engineering and Computing},
	author = {Sabatini, Professor A. M.},
	month = feb,
	year = {2005},
	keywords = {Biomedical Engineering, Computer Applications, Computer simulations, Gait analysis, Human Physiology, Imaging / Radiology, Inertial sensing, Neurosciences, Quaternion of rotation, Strap-down integration},
	pages = {94--101},
	file = {Full Text PDF:C\:\\Users\\SSepulveda\\AppData\\Roaming\\Zotero\\Zotero\\Profiles\\pyre7rqa.default\\zotero\\storage\\FS9ICZQP\\Sabatini - 2005 - Quaternion-based strap-down integration method for.pdf:application/pdf;Snapshot:C\:\\Users\\SSepulveda\\AppData\\Roaming\\Zotero\\Zotero\\Profiles\\pyre7rqa.default\\zotero\\storage\\TQCWT6WG\\BF02345128.html:text/html}
