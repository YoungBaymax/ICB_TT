	file = {IEEE Xplore Abstract Record:C\:\\Users\\SSepulveda\\AppData\\Roaming\\Zotero\\Zotero\\Profiles\\pyre7rqa.default\\zotero\\storage\\DN97SPS3\\articleDetails.html:text/html;IEEE Xplore Full Text PDF:C\:\\Users\\SSepulveda\\AppData\\Roaming\\Zotero\\Zotero\\Profiles\\pyre7rqa.default\\zotero\\storage\\A5NG9DXQ\\Suh et al. - 2012 - Quaternion-Based Indirect Kalman Filter Discarding.pdf:application/pdf}
