	pages = {2257--2266},
	file = {IEEE Xplore Full Text PDF:C\:\\Users\\SSepulveda\\AppData\\Roaming\\Zotero\\Zotero\\Profiles\\pyre7rqa.default\\zotero\\storage\\3ESBZG2P\\Bernard et al. - 2013 - MotionExplorer Exploratory Search in Human Motion.pdf:application/pdf}
