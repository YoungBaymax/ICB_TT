	month = sep,
	year = {2009},
	keywords = {acceleration, acceleration sensor, Accelerometers, Adult, ankle, back, biomechanics, Biosensing Techniques, Bruce protocol, Energy expenditure estimation, Energy Metabolism, exercise, Humans, knee, Male, regression analysis, respiratory gas, submaximal exercise, treadmills, triaxial accelerometers, Wrist},
